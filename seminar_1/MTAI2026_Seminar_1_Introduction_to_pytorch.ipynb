        "import torch\n",
        "from torchviz import make_dot\n",
        "\n",
        "x = torch.tensor(3.0, requires_grad=True)\n",
        "y = torch.tensor(4.0, requires_grad=True)\n",
        "c = torch.tensor(2.0, requires_grad=True)\n",
        "\n",
        "# Operations\n",
        "b = x * y\n",
        "a = (c ** b)\n",
        "z =  (a + b) * a\n",
        "\n",
        "# Visualize graph\n",
        "dot = make_dot(z, params={\"x\": x, \"y\": y, \"c\": c})\n",
        "\n",
        "dot\n"
