        "import torch\n",
        "from torchviz import make_dot\n",
        "\n",
        "# Create tensors\n",
        "x = torch.tensor(2.0, requires_grad=True)\n",
        "y = torch.tensor(3.0, requires_grad=True)\n",
        "\n",
        "# Simple operations\n",
        "z = x * y + y + 2\n",
        "\n",
        "# Visualize computation graph\n",
        "dot = make_dot(z, params={\"x\": x, \"y\": y})\n",
        "dot\n"
      ]
